{
  "name": "mb-react-tinymce",
  "version": "2.1.0",
  "author": "mockingbot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockingbot/mb-react-tinymce.git"
  },
  "main": "library/index.js",
  "scripts": {
    "example": "cross-env NODE_ENV=production webpack --watch --config config/example.conf.js",
    "example-watch": "cross-env NODE_ENV=development webpack --watch --config config/example.conf.js",
    "library": "cross-env NODE_ENV=production webpack --config config/library.conf.js",
    "library-watch": "cross-env NODE_ENV=production webpack --watch --config config/library.conf.js",
    "webpack-stats": "cross-env NODE_ENV=production webpack -p --config config/library.conf.js --json > stats.json",
    "generate-translation": "node __generate_translation__.js",
    "build": "npm run generate-translation && npm run library",
    "start": "npm run generate-translation && npm run example-watch",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "dev-dep-web-react-postcss": "^0.0.5",
    "messageformat": "^1.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-dom": "*"
  }
}
