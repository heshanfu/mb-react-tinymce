{
  "private": true,
  "name": "mb-react-tinymce",
  "version": "2.2.0",
  "author": "mockingbot",
  "license": "MIT",
  "repository": "https://github.com/mockingbot/mb-react-tinymce.git",
  "main": "library/index.js",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node ./script quiet",
    "script-pack": "babel-node ./script quiet pack",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "script-generate-spec": "babel-node ./script/generateSpec",
    "script-generate-t9n": "babel-node ./script/generateTranslation",
    "// build-dev ===================": "",
    "build-library-dev": "npm run build-library -- watch",
    "build-example-dev": "npm run build-example -- watch",
    "// build =======================": "",
    "build-library": "babel-node ./script/webpack production profile",
    "build-module": "babel-node ./script/webpack production profile module",
    "build-example": "babel-node ./script/webpack development example",
    "// =============================": "",
    "analyze-stat-library": "webpack-bundle-analyzer ./profile-stat-library-gitignore.json",
    "analyze-stat-module": "webpack-bundle-analyzer ./profile-stat-module-gitignore.json",
    "prepack": "echo \"Error: should run pack with script-*\" && exit 1"
  },
  "devDependencies": {
    "dev-dep-tool": "0.2.0-dev.2",
    "dev-dep-web-react-postcss": "0.2.0-dev.0",
    "messageformat": "^1.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*"
  }
}
